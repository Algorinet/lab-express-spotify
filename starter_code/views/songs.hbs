<div class="container">
    <div class="row header-row">
        <span class="col-lg-8">Title</span>
        <span class="col-lg-4">Listen</span>
    </div>
    {{#each songs}}
    <div>
        <figure>
            <div class="row track-item">
                <div class="col-lg-8">
                    <figcaption>{{this.name}}</figcaption>
                </div>
                <div class="col-lg-4">
                    <audio controls src={{this.preview_url}}>
                        Your browser does not support the
                        <code>audio</code> element.
                    </audio>
        </figure>
    </div>

    {{/each}}
    </div>